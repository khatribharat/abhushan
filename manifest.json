{
	"manifest_version": 2,

	"name": "Abhushan",
	"version": "0.1",
	"description": "Abhushan lets you style websites as you like.",
	"permissions": ["https://news.ycombinator.com/*"],
	"content_scripts": [
		{
			"matches": ["https://news.ycombinator.com/*"],
			"css": ["styles/news.ycombinator.com/style.css"]
		},
		{
			"matches": ["http://itsalmo.st/*"],
			"css": ["styles/itsalmo.st/style.css"]
		}
	]  
}
